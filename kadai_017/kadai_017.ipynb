        "class Human:\n",
        "  def __init__(self,name :str, age :int):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def checkadult(self):\n",
        "    if self.age >= 20:\n",
        "      print(f\"{self.name}さんは、大人です\")\n",
        "    else:\n",
        "      print(f\"{self.name}さんは、大人ではありません\")\n",
        "\n",
        "user = Human(\"侍太郎\",36)\n",
        "user.checkadult()\n"
      ]
    }
  ]
